<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:webViewControl="clr-namespace:WebViewControl;assembly=WebViewControl.Avalonia"
			 mc:Ignorable="d"
			 x:Class="ChatModApp.Views.AuthenticationView">
	<Grid RowDefinitions="Auto,*">
		<TextBlock Text="Please authenticate on Twitch to continue"/>
		<webViewControl:WebView Grid.Row="1" x:Name="AuthWebView"/>
	</Grid>
</UserControl>
